{
  "name": "vibe_studio",
  "private": true,
  "version": "1.0.2",
  "productName": "VibeStudio",
  "description": "VibeStudio",
  "author": {
    "name": "Nidum",
    "email": "info@nidum.ai"
  },
  "main": "./dist-electron/index.js",
  "type": "module",
  "scripts": {
    "start": "vite dev",
    "dev": "vite dev",
    "preinstall": "npm install --cpu=wasm32 sharp --no-save",
    "prebuild": "rimraf ./dist ./dist-electron ./release",
    "build": "tsc && vite build && electron-builder",
    "build-win": "npm run prebuild && tsc && vite build && electron-builder --win --x64",
    "build-mas": "npm run prebuild && ./scripts/install-cert.sh && tsc && vite build && electron-builder --mac mas",
    "clean": "rm -rf ./node_modules ./dist ./dist-electron ./release ./models"
  },
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
    "@codemirror/lang-javascript": "^6.2.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.3.2",
    "@codemirror/lang-python": "^6.2.0",
    "@hookform/resolvers": "^3.10.0",
    "@huggingface/transformers": "^3.5.2",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-collapsible": "^1.1.8",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.4",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^5.74.4",
    "@uiw/codemirror-theme-dracula": "^4.23.12",
    "@uiw/codemirror-theme-vscode": "^4.23.10",
    "@uiw/react-codemirror": "^4.23.10",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-web-links": "^0.11.0",
    "@xterm/xterm": "^5.5.0",
    "adm-zip": "^0.5.16",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "detect-port": "^2.1.0",
    "dotenv": "^16.5.0",
    "electron-log": "^5.2.4",
    "electron-updater": "^6.3.9",
    "express": "^5.1.0",
    "kokoro-js": "^1.2.1",
    "lucide-react": "^0.503.0",
    "nanoid": "^5.1.5",
    "next-themes": "^0.4.6",
    "node-pty": "^1.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.2",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "systeminformation": "^5.25.11",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tar": "^7.4.3",
    "winston": "^3.17.0",
    "xterm": "^5.3.0",
    "xterm-addon-ligatures": "^0.7.0",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@types/adm-zip": "^0.5.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.7.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.3.1",
    "electron": "^32.1.0",
    "electron-builder": "^24.13.3",
    "rimraf": "^6.0.1",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.6.2",
    "vite": "^5.4.5",
    "vite-plugin-electron": "^0.28.7",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-svgr": "^4.2.0"
  },
  "publish": {
    "provider": "generic",
    "url": "https://releases.nidum.ai/vibestudio/"
  },
  "build": {
    "appId": "com.nidum.vibestudio",
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "public/bin/",
        "to": "bin/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "**/bin/**"
    ],
    "asar": true,
    "directories": {
      "output": "dist",
      "buildResources": "public"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip",
        "pkg"
      ],
      "hardenedRuntime": true,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.inherit.plist",
      "provisioningProfile": "Mac_App_Store_Connect.provisionprofile",
      "gatekeeperAssess": false,
      "notarize": {
        "teamId": "JCN5972XNZ"
      }
}}}